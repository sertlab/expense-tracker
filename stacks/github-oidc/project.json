{
  "name": "github-oidc",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "stacks/github-oidc",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "options": { "cwd": "stacks/github-oidc", "command": "serverless deploy --verbose" }
    },
    "remove": {
      "executor": "nx:run-commands",
      "options": { "cwd": "stacks/github-oidc", "command": "serverless remove --verbose" }
    }
  }
}
